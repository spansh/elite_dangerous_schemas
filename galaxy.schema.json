{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://dowloads.spansh.co.uk/schema/galaxy.schema.json",
  "title": "Spansh Galaxy",
  "description": "Full Galaxy files downloaded from https://spansh.co.uk/downloads",
  "$defs": {
    "Power": {
      "type": [
        "null",
        "string"
      ],
      "enum": ["Aisling Duval","A. Lavigny-Duval","Archon Delaine","Denton Patreus","Edmund Mahon","Felicia Winters","Jerome Archer","Li Yong-Rui","Nakato Kaine","Pranav Antal","Yuri Grom","Zemina Torval"]
    },
    "PowerState": {
      "type": [
        "null",
        "string"
      ],
      "enum": [null,"Exploited","Fortified","Stronghold","Unoccupied"]
    },
    "FactionState": {
      "type": "string",
      "enum": [null,"Blight","Boom","Bust","Civil Liberty","Civil Unrest","Civil War","Drought","Election","Expansion","Famine","Infrastructure Failure","Investment","Lockdown","Natural Disaster","None","Outbreak","Pirate Attack","Public Holiday","Retreat","Terrorist Attack","War"]
    },
    "Government": {
      "type": [
        "null",
        "string"
      ],
      "enum": [null,"Anarchy","Communism","Confederacy","Cooperative","Corporate","Democracy","Dictatorship","Engineer","Feudal","Megaconstruction","None","Patronage","Prison","Prison Colony","Private Ownership","Theocracy"]
    },
    "Allegiance": {
      "type": [
        "null",
        "string"
      ],
      "enum": [null,"Alliance","Empire","Federation","Guardian","Independent","Pilots Federation","Thargoid"]
    },
    "Economy": {
      "type": [
        "null",
        "string"
      ],
      "enum": [null,"Agriculture","Colony","Extraction","High Tech","Industrial","Military","None","Prison","Private Enterprise","Refinery","Repair","Rescue","Service","Terraforming","Tourism"]
    },
    "Timestamps": {
      "description": "Update timestamps for individual attributes, mapping attribute to last updated timestamp.",
      "type": "object",
      "patternProperties": {
        ".": {
          "type": "string",
          "format": "date-time"
        }
      },
      "additionalProperties": false
    },
    "ValueMapping": {
      "type": "object",
      "patternProperties": {
        ".": {
          "type": "number"
        }
      },
      "additionalProperties": false
    },
    "BeltOrRing": {
      "type": "object",
      "properties": {
        "name": {
          "description": "Name of the belt or ring.",
          "type": "string"
        },
        "type": {
          "description": "Type of the ring or belt.",
          "type": "string",
          "enum": ["Icy","Metal Rich","Metallic","Rocky"]
        },
        "mass": {
          "description": "Mass of the belt or ring, measured in Megatonnes.",
          "type": "number",
          "minimum": 0
        },
        "innerRadius": {
          "description": "Inner radius of the belt or ring, measured in Kilometres.",
          "type": "number",
          "minimum": 0
        },
        "outerRadius": {
          "description": "Outer radius of the belt or ring, measured in Kilometres.",
          "type": "number",
          "minimum": 0
        },
        "id64": {
          "description": "ID64 of the belt or ring, a 64 bit unsigned integer.",
          "type": "integer",
          "minimum": 0
        },
        "signals": {
          "description": "The signals that can be found within the belt or ring.",
          "type": "object",
          "properties": {
            "signals": {
              "$ref": "#/$defs/ValueMapping"
            },
            "updateTime": {
              "description": "The timestamp that the signals were last updated, in UTC.",
              "type": "string",
              "format": "date-time"
            }
          },
          "required": [
            "signals",
            "updateTime"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "name",
        "type",
        "mass",
        "innerRadius",
        "outerRadius"
      ],
      "additionalProperties": false
    },
    "Body": {
      "type": "object",
      "properties": {
        "id64": {
          "description": "The ID64 of the body, a 64 bit unsigned integer.",
          "type": "integer",
          "minimum": 0
        },
        "bodyId": {
          "description": "The numeric ID of the body within the system, a 64 bit unsigned integer.",
          "type": "integer",
          "minimum": 0
        },
        "name": {
          "description": "The name of the body.",
          "type": "string"
        },
        "type": {
          "description": "The type of the body.",
          "type": "string",
          "enum": ["Planet","Star","Barycentre"]
        },
        "orbitalPeriod": {
          "description": "The orbital period of the body, measured in days.",
          "type": "number"
        },
        "semiMajorAxis": {
          "description": "The semi major axis of the body, measured in Kilometers.",
          "type": "number"
        },
        "orbitalEccentricity": {
          "description": "The orbital eccentricity of the body.",
          "type": "number",
          "minimum": 0,
          "maximum": 1
        },
        "orbitalInclination": {
          "description": "The orbital inclinction of the body, measured in degrees.",
          "type": "number"
        },
        "argOfPeriapsis": {
          "description": "The argument of periapsis the body, measured in degrees.",
          "type": "number",
          "minimum": 0,
          "maximum": 360
        },
        "meanAnomaly": {
          "description": "The mean of anomaly the body, measured in degrees.",
          "type": "number",
          "minimum": 0,
          "maximum": 360
        },
        "ascendingNode": {
          "description": "The ascending node of the body, measured in degrees.",
          "type": "number",
          "minimum": -180,
          "maximum":180 
        },
        "timestamps": {
          "description": "Timestamps of the last update of some of the fields within the body.",
          "$ref": "#/$defs/Timestamps"
        },
        "stations": {
          "description": "Stations which are located on the body, planet based stations.",
          "type": "array",
          "items": {
            "$ref": "#/$defs/Station"
          }
        },
        "updateTime": {
          "description": "Timestamp of the last update of the body, in UTC.",
          "type": "string"
        },
        "subType": {
          "description": "Subtype of the body.",
          "type": "string",
          "enum": ["A (Blue-White super giant) Star","A (Blue-White) Star","Ammonia world","B (Blue-White super giant) Star","B (Blue-White) Star","Black Hole","C Star","CJ Star","CN Star","Class I gas giant","Class II gas giant","Class III gas giant","Class IV gas giant","Class V gas giant","Earth-like world","F (White super giant) Star","F (White) Star","G (White-Yellow super giant) Star","G (White-Yellow) Star","Gas giant with ammonia-based life","Gas giant with water-based life","Helium gas giant","Helium-rich gas giant","Herbig Ae\/Be Star","High metal content world","Icy body","K (Yellow-Orange giant) Star","K (Yellow-Orange) Star","L (Brown dwarf) Star","M (Red dwarf) Star","M (Red giant) Star","M (Red super giant) Star","MS-type Star","Metal-rich body","Neutron Star","O (Blue-White) Star","Rocky Ice world","Rocky body","S-type Star","Supermassive Black Hole","T (Brown dwarf) Star","T Tauri Star","Water giant","Water world","White Dwarf (D) Star","White Dwarf (DA) Star","White Dwarf (DAB) Star","White Dwarf (DAV) Star","White Dwarf (DAZ) Star","White Dwarf (DB) Star","White Dwarf (DBV) Star","White Dwarf (DBZ) Star","White Dwarf (DC) Star","White Dwarf (DCV) Star","White Dwarf (DQ) Star","Wolf-Rayet C Star","Wolf-Rayet N Star","Wolf-Rayet NC Star","Wolf-Rayet O Star","Wolf-Rayet Star","Y (Brown dwarf) Star"]
        },
        "distanceToArrival": {
          "description": "Distance to the system arrival point of the body, measured in Kilometres.",
          "type": "number"
        },
        "mainStar": {
          "description": "Stars only.  Whether this is the main star of the system or not.",
          "type": "boolean"
        },
        "age": {
          "description": "Stars only.  The age of the star, measured in solar years.",
          "type": "integer",
          "minimum": 0
        },
        "spectralClass": {
          "description": "Stars only.  Spectral class of the star.",
          "type": [
            "string",
            "null"
          ],
          "enum": [null,"A","A0","A1","A2","A3","A4","A5","A6","A7","A8","A9","AeBe0","AeBe1","AeBe2","AeBe3","AeBe4","AeBe5","AeBe6","AeBe7","AeBe8","AeBe9","B","B0","B1","B2","B3","B4","B5","B6","B7","B8","B9","C0","C1","C2","C3","C4","C5","C6","C7","C8","C9","CJ4","CJ5","CJ6","CN4","CN5","CN6","DA0","DAB0","DAV0","DAZ0","DB0","DC0","F","F0","F1","F2","F3","F4","F5","F6","F7","F8","F9","G","G0","G1","G2","G3","G4","G5","G6","G7","G8","G9","H0","K","K0","K1","K2","K3","K4","K5","K6","K7","K8","K9","L","L0","L1","L2","L3","L4","L5","L6","L7","L8","L9","M","M0","M1","M2","M3","M4","M5","M6","M7","M8","M9","MS0","MS1","MS2","MS3","MS4","MS5","MS6","O","O0","O1","O2","O3","O4","O5","O6","O7","O8","O9","S0","S1","S2","S3","S4","S5","S6","S7","T","T0","T1","T2","T3","T4","T5","T6","T7","T8","T9","TTS0","TTS1","TTS2","TTS3","TTS4","TTS5","TTS6","TTS7","TTS8","TTS9","W1","W2","W3","W4","W5","W6","W7","W8","W9","WC0","WC2","WC5","WC6","WN0","WN3","WN5","WNC0","WO0","Y0","Y1","Y2","Y3","Y4","Y5","Y6","Y9"]
        },
        "luminosity": {
          "description": "Stars only.  Luminosity of the star.",
          "type": "string",
          "enum": ["I","II","III","IIIa","IIIab","IIIb","IIa","IIab","IIb","IV","IVa","IVab","IVb","Ia","Ia0","Iab","Ib","O","V","VI","VII","Va","Vab","Vb","Vz"]
        },
        "absoluteMagnitude": {
          "description": "Stars only.  Absolute magnitude of the star.",
          "type": "number"
        },
        "solarMasses": {
          "description": "Stars only. Mass of the star, as a ratio of the mass of Sol.",
          "type": "number",
          "minimum": 0
        },
        "solarRadius": {
          "description": "Stars only. Radius of the star, as a ratio of the radius of Sol.",
          "type": "number",
          "minimum": 0
        },
        "surfaceTemperature": {
          "description": "Surface temperature of the body, measured in Kelvin.",
          "type": "number",
          "minimum": 0
        },
        "rotationalPeriod": {
          "description": "Rotational period of the body, measured in Days.",
          "type": "number"
        },
        "rotationalPeriodTidallyLocked": {
          "description": "Whether the rotational period of the body is locked to the tides.",
          "type": "boolean"
        },
        "axialTilt": {
          "description": "Axial tilt of the body, measured in radians.",
          "type": "number"
        },
        "parents": {
          "description": "Parents of the body, mapping each parent body type to it's ID within the system.",
          "type": "array",
          "items": {
            "$ref": "#/$defs/ValueMapping"
          }
        },
        "isLandable": {
          "description": "Planets only.  Whether the planet is landable or not.",
          "type": "boolean"
        },
        "gravity": {
          "description": "Planets only.  Gravity of the planet, as a ratio of the gravity of Earth.",
          "type": "number",
          "minimum": 0
        },
        "earthMasses": {
          "description": "Planets only.  Mass of the planet, as a ratio of the mass of Earth.",
          "type": "number",
          "minimum": 0
        },
        "radius": {
          "description": "Planets only.  Radius of the planet, measured in Kilimetres.",
          "type": "number",
          "minimum": 0
        },
        "surfacePressure": {
          "description": "Planets only.  Surface pressure of the planet, measured in atmospheres.",
          "type": "number",
          "minimum": 0
        },
        "volcanismType": {
          "description": "Planets only.  Volcanism type of the planet.",
          "type": "string",
          "enum": ["Carbon Dioxide Geysers","Major Carbon Dioxide Geysers","Major Metallic Magma","Major Rocky Magma","Major Silicate Vapour Geysers","Major Water Geysers","Major Water Magma","Metallic Magma","Minor Ammonia Magma","Minor Carbon Dioxide Geysers","Minor Metallic Magma","Minor Methane Magma","Minor Nitrogen Magma","Minor Rocky Magma","Minor Silicate Vapour Geysers","Minor Water Geysers","Minor Water Magma","No volcanism","Rocky Magma","Silicate Vapour Geysers","Water Geysers","Water Magma"]
        },
        "atmosphereType": {
          "description": "Planets only.  Atmosphere type of the planet.",
          "type": [
            "string",
            "null"
          ],
          "enum": [null,"Ammonia","Ammonia and Oxygen","Ammonia-rich","Argon","Argon-rich","Carbon dioxide","Carbon dioxide-rich","Helium","Hot Argon","Hot Argon-rich","Hot Carbon dioxide","Hot Carbon dioxide-rich","Hot Metallic vapour","Hot Silicate vapour","Hot Sulphur dioxide","Hot Water","Hot Water-rich","Hot thick Ammonia","Hot thick Ammonia-rich","Hot thick Argon","Hot thick Argon-rich","Hot thick Carbon dioxide","Hot thick Carbon dioxide-rich","Hot thick Metallic vapour","Hot thick Methane","Hot thick Methane-rich","Hot thick Nitrogen","Hot thick Silicate vapour","Hot thick Sulphur dioxide","Hot thick Water","Hot thick Water-rich","Hot thin Carbon dioxide","Hot thin Metallic vapour","Hot thin Silicate vapour","Hot thin Sulphur dioxide","Methane","Methane-rich","Neon-rich","Nitrogen","No atmosphere","Oxygen","Suitable for water-based life","Sulphur dioxide","Thick Ammonia","Thick Ammonia and Oxygen","Thick Ammonia-rich","Thick Argon","Thick Argon-rich","Thick Carbon dioxide","Thick Carbon dioxide-rich","Thick Helium","Thick Methane","Thick Methane-rich","Thick Nitrogen","Thick No atmosphere","Thick Suitable for water-based life","Thick Sulphur dioxide","Thick Water","Thick Water-rich","Thin Ammonia","Thin Ammonia and Oxygen","Thin Ammonia-rich","Thin Argon","Thin Argon-rich","Thin Carbon dioxide","Thin Carbon dioxide-rich","Thin Helium","Thin Methane","Thin Methane-rich","Thin Neon","Thin Neon-rich","Thin Nitrogen","Thin Oxygen","Thin Sulphur dioxide","Thin Water","Thin Water-rich","Water","Water-rich"]
        },
        "atmosphereComposition": {
          "description": "Planets only.  Atmosphere composition of the planet, mapping each element to it's percentage.",
          "$ref": "#/$defs/ValueMapping"
        },
        "solidComposition": {
          "description": "Planets only.  Solid composition of the planet, mapping each element to it's percentage.",
          "$ref": "#/$defs/ValueMapping"
        },
        "terraformingState": {
          "description": "Planets only.  terraforming state of the planet.",
          "type": "string",
          "enum": ["Not terraformable","Terraformable","Terraformed","Terraforming"]
        },
        "materials": {
          "description": "Planets only.  Materials which can be found on the planet, mapping each material to it's percentage.",
          "$ref": "#/$defs/ValueMapping"
        },
        "signals": {
          "description": "Planets only.  The signals that can be found on the planet.",
          "type": "object",
          "properties": {
            "signals": {
              "$ref": "#/$defs/ValueMapping"
            },
            "updateTime": {
              "description": "The timestamp that the signals were last updated, in UTC.",
              "type": "string",
              "format": "date-time"
            },
            "genuses": {
              "description": "Genuses which can be found on this planet.",
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["$Codex_Ent_Aleoids_Genus_Name;","$Codex_Ent_Bacterial_Genus_Name;","$Codex_Ent_Brancae_Name;","$Codex_Ent_Cactoid_Genus_Name;","$Codex_Ent_Clypeus_Genus_Name;","$Codex_Ent_Conchas_Genus_Name;","$Codex_Ent_Cone_Name;","$Codex_Ent_Electricae_Genus_Name;","$Codex_Ent_Fonticulus_Genus_Name;","$Codex_Ent_Fumerolas_Genus_Name;","$Codex_Ent_Fungoids_Genus_Name;","$Codex_Ent_Ground_Struct_Ice_Name;","$Codex_Ent_Osseus_Genus_Name;","$Codex_Ent_Recepta_Genus_Name;","$Codex_Ent_Shrubs_Genus_Name;","$Codex_Ent_Sphere_Name;","$Codex_Ent_Stratum_Genus_Name;","$Codex_Ent_Tubus_Genus_Name;","$Codex_Ent_Tussocks_Genus_Name;"]
              }
            }
          },
          "additionalProperties": false
        },
        "reserveLevel": {
          "description": "Planets only.  Reserve level of the planet.",
          "type": "string",
          "enum": ["Common","Depleted","Low","Major","Pristine"]
        },
        "rings": {
          "description": "Rings associated with the body.",
          "type": "array",
          "items": {
            "$ref": "#/$defs/BeltOrRing"
          }
        },
        "belts": {
          "description": "Belts associated with the body.",
          "type": "array",
          "items": {
            "$ref": "#/$defs/BeltOrRing"
          }
        }
      },
      "required": [
        "id64",
        "bodyId",
        "name",
        "updateTime"
      ],
      "additionalProperties": false
    },
    "Station": {
      "type": "object",
      "properties": {
        "name": {
          "description": "Name of the station",
          "type": "string"
        },
        "id": {
          "description": "Market ID of the station.",
          "type": "integer",
          "minimum": 0
        },
        "updateTime": {
          "description": "Timestamp of the last update of the station, in UTC.",
          "type": "string",
          "format": "date-time"
        },
        "realName": {
          "description": "Real name of the station, for colonisation stations.",
          "type": "string"
        },
        "carrierName": {
          "description": "Player given name of the station, for fleet carriers.",
          "type": "string"
        },
        "controllingFaction": {
          "description": "Controlling faction of the station.",
          "type": "string"
        },
        "controllingFactionState": {
          "description": "Statre of the controlling faction of the station.",
          "type": [
            "null",
            "string"
          ],
          "enum": [null,"Blight","Boom","Bust","Civil Liberty","Civil Unrest","Civil War","Drought","Election","Expansion","Famine","Infrastructure Failure","Investment","Lockdown","Natural Disaster","Outbreak","Pirate Attack","Public Holiday","Retreat","Terrorist Attack","War"]
        },
        "distanceToArrival": {
          "description": "Distance to the system arrival point of the station, measured in Kilometres.",
          "type": "number"
        },
        "primaryEconomy": {
          "description": "Primary economy for the station.",
          "$ref": "#/$defs/Economy"
        },
        "secondaryEconomy": {
          "description": "Secondary economy for the station.",
          "$ref": "#/$defs/Economy"
        },
        "economies": {
          "description": "All econommies for the station, mapping each economy as a proportion of the stations economies.",
          "$ref": "#/$defs/ValueMapping"
        },
        "allegiance": {
          "description": "Allegiance of the station.",
          "$ref": "#/$defs/Allegiance"
        },
        "government": {
          "description": "Government type of the station.",
          "$ref": "#/$defs/Government"
        },
        "services": {
          "description": "Servies provided by the station.",
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["Flight Controller","Station Operations","Dock","Autodock","Station Menu","Contacts","Refuel","Market","Repair","Workshop","Missions","Search and Rescue","Missions Generated","Restock","Social Space","Black Market","Interstellar Factors Contact","Powerplay","System Colonisation","Crew Lounge","Universal Cartographics","Outfitting","Livery","Construction Services","Shop","Bartender","Vista Genomics","Shipyard","Pioneer Supplies","Apex Interstellar","Tuning","Fleet Carrier Fuel","Fleet Carrier Management","Frontline Solutions","Refinery Contact","Redemption Office","Technology Broker","Material Trader","Squadron Bank","Fleet Carrier Administration","On Dock Mission","Fleet Carrier Vendor"]
          }
        },
        "type": {
          "description": "Type of the station.",
          "type": "string",
          "enum": ["Asteroid base","Coriolis Starport","Dockable Planet Station","Dodec Starport","Drake-Class Carrier","Mega ship","Ocellus Starport","Orbis Starport","Outpost","Planetary Construction Depot","Planetary Outpost","Planetary Port","Settlement","Space Construction Depot","Surface Settlement"]
        },
        "state": {
          "description": "State of the station.",
          "type": "string",
          "enum": ["Construction","Damaged","UnderAttack","UnderRepairs"]
        },
        "latitude": {
          "description": "Planetary stations only.  Latitude of the station.",
          "type": "number"
        },
        "longitude": {
          "description": "Planetary stations only.  Longitude of the station.",
          "type": "number"
        },
        "landingPads": {
          "description": "Breakdown of the number of landing pads of the station.",
          "type": "object",
          "properties": {
            "large": {
              "description": "Numer of large landing pads of the station.",
              "type": "integer",
              "minimum": 0
            },
            "medium": {
              "description": "Numer of medium landing pads of the station.",
              "type": "integer",
              "minimum": 0
            },
            "small": {
              "description": "Numer of small landing pads of the station.",
              "type": "integer",
              "minimum": 0
            }
          },
          "required": [
            "large",
            "medium",
            "small"
          ],
          "additionalProperties": false
        },
        "carrierDockingAccess": {
          "description": "Carrier only.  Access control of the station.",
          "type": "string"
        },
        "market": {
          "description": "Commodity market information about the station.",
          "type": "object",
          "properties": {
            "commodities": {
              "description": "Commodities that can be bought and sold at about the station.",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "name": {
                    "description": "Name of the commodity.",
                    "type": "string"
                  },
                  "symbol": {
                    "description": "Frontier symbol identifier of the commodity.",
                    "type": "string"
                  },
                  "category": {
                    "description": "Category of the commodity.",
                    "type": "string",
                    "enum": ["Chemicals","Consumer Items","Foods","Industrial Materials","Legal Drugs","Machinery","Medicines","Metals","Minerals","Salvage","Slavery","Technology","Textiles","Waste","Weapons"]
                    },
                  "commodityId": {
                    "description": "Frontier numeric identifier of the commodity.",
                    "type": "integer"
                  },
                  "demand": {
                    "description": "Demand for the commodity.",
                    "type": "integer",
                    "minimum": 0
                  },
                  "supply": {
                    "description": "Supply for the commodity.",
                    "type": "integer",
                    "minimum": 0
                  },
                  "buyPrice": {
                    "description": "Price the player can buy the commodity for.",
                    "type": "integer",
                    "minimum": 0
                  },
                  "sellPrice": {
                    "description": "Price the player can sell the commodity for.",
                    "type": "integer",
                    "minimum": 0
                  }
                },
                "required": [
                  "name",
                  "symbol",
                  "category",
                  "commodityId",
                  "demand",
                  "supply",
                  "buyPrice",
                  "sellPrice"
                ],
                "additionalProperties": false
              }
            },
            "prohibitedCommodities": {
              "description": "Commodities that are illegal on the station.",
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "updateTime": {
              "description": "Timestamp of the last update of the marketplace for the station, in UTC.",
              "type": "string",
              "format": "date-time"
            }
          },
          "required": [
            "commodities",
            "prohibitedCommodities",
            "updateTime"
          ],
          "additionalProperties": false
        },
        "shipyard": {
          "description": "Shipyard information about the station.",
          "type": "object",
          "properties": {
            "ships": {
              "description": "Ships that can be bought at the station.",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "name": {
                    "description": "Name of the ship.",
                    "type": "string"
                  },
                  "symbol": {
                    "description": "Frontier symbol identifier of the ship.",
                    "type": "string"
                  },
                  "shipId": {
                    "description": "Frontier numeric identifier of the ship.",
                    "type": "number"
                  }
                },
                "required": [
                  "name",
                  "symbol",
                  "shipId"
                ],
                "additionalProperties": false
              }
            },
            "updateTime": {
              "description": "Timestamp of the last update of the shipyard for the station, in UTC.",
              "type": "string",
              "format": "date-time"
            }
          },
          "required": [
            "ships",
            "updateTime"
          ],
          "additionalProperties": false
        },
